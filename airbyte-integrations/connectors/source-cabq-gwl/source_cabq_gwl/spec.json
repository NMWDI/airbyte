{
  "documentationUrl": "https://docsurl.com",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Cabq Gwl Spec",
    "type": "object",
    "required": ["email_address",
                "email_password",
                "host", "port"],
    "additionalProperties": false,
    "properties": {
      "email_address": {
        "type": "string",
        "description": "describe me"
      },
      "email_password": {
        "type": "string",
        "description": "Password for 'email_address'",
        "airbyte_secret": true
      },
      "email_subject": {
        "type": "string",
        "description": "find emails with subject that match 'email_subject'"
      },
      "host": {
        "type": "string",
        "description": "IMAP host"
      },
      "port": {
        "type": "integer",
        "description": "IMAP port'"
      }
    }
  }
}
